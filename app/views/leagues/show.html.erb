<div class="col-lg-10 col-lg-offset-1">
	
	<table class="table table-striped table-condensed">
		<thead>
			<tr>
				<th colspan="2"><%= @league.name%></th><th colspan="2"><%= @league.positions %></th>
			</tr>
			
			<tr>
				<th>Team Name</th>
				<th>Owner</th>
				<th>Points</th>
				<th>link</th>
			</tr>
			<tbody>
				<% @league.fantasy_teams.each do |f| %>
					<tr>
						<td><%= f.name %></td>
						<td><%= f.user.email %></td>
						<td><%= f.current_fantasy_total %></td>
						<td><%= link_to "show", fantasy_team_path(f) %></td>
					</tr>
				<% end %>
			</tbody>
		</thead>
	</table>
	
</div>